    "push": "apify push"
  },
  "keywords": [
    "real-estate",
    "germany",
    "immobilienscout24",
    "immonet",
    "immowelt",
    "property-aggregation",
    "deduplication",
    "scraping",
    "apify"
  ],
  "author": "Barrierefix <kontakt@barrierefix.de>",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "apify": "^3.4.5",
    "crawlee": "^3.15.0",
    "zod": "^4.1.11",
    "luxon": "^3.7.2",
    "playwright": "^1.40.0",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/luxon": "^3.4.2",
    "typescript": "^5.9.2",
    "jest": "^30.1.3",
    "@types/jest": "^30.0.0",
    "ts-node": "^10.9.2"
  }
